<ul class="breadcrumb">
<li class="active">
WordsEye Control Panel
</li>
</ul>



<div class="row-fluid">
<table class="table table-striped table-bordered table-condensed">
	<thead>
	<tr>
	<th>Hit</th>
	<th>Url</th>
	<th>Date</th>
	</tr>
	</thead>
	<tbody>
	<% @hits.each do |hit|%>
	<tr>
	<td><%= hit.title %></td>
	<td><%= link_to hit.url, hit.url %></td>
	<td><%= time_ago hit.created_at %></td>
	</tr>
	<% end %>
	</tbody>
</table>

</div>
<% content_for :sidebar do %>
<% unless signed_in? %> 
<span style="display:none;">&nbsp;</span>
<% end %>
<% end %>